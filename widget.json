{
  "id": "com.liorbenisty.hebrew.table.v2",
  "name": "Hebrew Dynamic Table",
  "description": "Dynamic Hebrew table with RTL support",
  "version": "2.0.0",
  "newInstancePrefix": "HebrewTable",
  "vendor": "liorbenisty.com",
  "license": "MIT",
  "imports": [
    "datasource"
  ],
  "webcomponents": [
    {
      "kind": "main",
      "tag": "dynamic-hebrew-table",
      "url": "https://liorbenistyy.github.io/sac-rtl-table-widget/table-widget.js",
      "integrity": "",
      "ignoreIntegrity": true
    },
    {
      "kind": "builder",
      "tag": "dynamic-hebrew-table-builder",
      "url": "https://liorbenistyy.github.io/sac-rtl-table-widget/table-widget.js",
      "integrity": "",
      "ignoreIntegrity": true
    }
  ],
  "properties": {
    "tableTitle": {
      "type": "string",
      "description": "Table title",
      "default": "טבלה דינמית"
    },
    "rtlMode": {
      "type": "boolean",
      "description": "RTL mode",
      "default": true
    }
  },
  "methods": {},
  "events": {},
  "dataBindings": {
    "myBinding": {
      "feeds": [
        {
          "id": "dimensions",
          "type": "dimension"
        },
        {
          "id": "measures",
          "type": "mainStructureMember"
        }
      ]
    }
  }
}
