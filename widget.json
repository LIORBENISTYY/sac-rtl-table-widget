{
  "id": "com.liorbenisty.minimal.table.v1",
  "name": "Minimal Hebrew Table",
  "description": "Minimal dynamic Hebrew table for testing",
  "version": "1.0.0",
  "newInstancePrefix": "MinimalTable",
  "vendor": "liorbenisty.com",
  "license": "MIT",
  "imports": [
    "datasource"
  ],
  "webcomponents": [
    {
      "kind": "main",
      "tag": "minimal-dynamic-table",
      "url": "https://liorbenistyy.github.io/sac-rtl-table-widget/table-widget.js",
      "integrity": "",
      "ignoreIntegrity": true
    },
    {
      "kind": "builder",
      "tag": "minimal-dynamic-table-builder",
      "url": "https://liorbenistyy.github.io/sac-rtl-table-widget/table-widget.js",
      "integrity": "",
      "ignoreIntegrity": true
    }
  ],
  "properties": {
    "tableTitle": {
      "type": "string",
      "description": "Table title",
      "default": "טבלה דינמית"
    },
    "rtlMode": {
      "type": "boolean",
      "description": "RTL mode",
      "default": true
    }
  },
  "methods": {},
  "events": {},
  "dataBindings": {
    "myBinding": {
      "feeds": [
        {
          "id": "dimensions",
          "type": "dimension"
        },
        {
          "id": "measures",
          "type": "mainStructureMember"
        }
      ]
    }
  }
}
