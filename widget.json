{
  "id": "com.sap.sac.sample.tablewidget",
  "name": "Table Widget",
  "description": "A Custom Widget to display tabular data from SAC",
  "vendor": "SAP",
  "version": "1.0.0",
  "icon": "",
  "webcomponents": [
    {
      "kind": "main",
      "tag": "github-sap-table-widget",
      "url": "/tableWidget.js",
      "integrity": "",
      "ignoreIntegrity": true
    }
  ],
  "properties": {
    "width": { "type": "integer", "default": 600 },
    "height": { "type": "integer", "default": 400 },
    "caption": {
      "type": "string",
      "description": "Table title",
      "default": ""
    }
  },
  "dataBindings": {
    "myDataSource": {
      "feeds": [
        { "id": "dimensions", "type": "dimension" },
        { "id": "measures", "type": "mainStructureMember" }
      ]
    }
  },
  "methods": {
    "getDataSource": {
      "parameters": [],
      "returnType": "DataSource",
      "body": "return this.dataBindings.getDataBinding('myDataSource').getDataSource();"
    }
  },
  "events": {
    "onResultChanged": { "description": "called when the data changes" }
  }
}
